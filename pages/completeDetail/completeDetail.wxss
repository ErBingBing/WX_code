  background: #fff;
}

.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  align-items: center;
  border-bottom: 1px solid #ccc;
  font-size: 14px;
}

.td-1 {
  width: 27%;
  justify-content: center;
  display: flex;
  text-align: right;
  padding-left: 0px;
  margin-left: 0px;
  background: #ececec;
  padding: 32rpx 0;
  height: 100%;
  line-height: 100%;
}

.td-2 {
  width: 73%;
  justify-content: center;
  text-align: center;
  max-width: 100%;
  height: 100%;
  line-height: 100%;
}